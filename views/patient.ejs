<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">

<h1>
	paitent page
</h1>

<div class="row"> 
	<% clinic.forEach(function(clinic){ %>   
	<div class="col-md-3 col-sm-6 profile "> 

<div class="card" style="width: 18rem;">
	<div class="card-body">
	  <h5 class="card-title"><%= clinic.clinicName %></h5>
	  <h6 class="card-subtitle mb-2 text-muted"><%= clinic.category %></h6>
	  <p class="card-text">this clinic is located in <%= clinic.address %>, <%= clinic.city %>. It charges <%= clinic.fees %> </p>
	  <a href="#" class="card-link"> take appointment</a>
	  <a href="#" class="card-link"> track live patients</a>
	</div>
  </div>
</div>
	<% }) %>
</div>